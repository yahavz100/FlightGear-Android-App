<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.javaflightgearandroidapp">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_launcher_iconbadge"
        android:label="FlightGear Controller"
        android:roundIcon="@mipmap/app_launcher_iconbadge_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JavaFlightGearAndroidApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>